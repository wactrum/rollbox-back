    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/throttler": "^4.0.0",
